{
    "name": "stejsky/composer-installer",
    "description": "Basic Composer Installer for Nette",
    "type": "composer-plugin",
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0",
        "composer-plugin-api": "1.0.0"
    },
    "extra": {
        "class": "Stejsky\\Composer\\NetteInstaller"
    },
    "autoload": {
        "psr-0": {"Tlapnet\\Composer": ""}
    },
    "extra": {
        "class": "Tlapnet\\Composer\\TlapnetModuleComposerInstaller"
    },
    "target-dir": "composer-installer"
}